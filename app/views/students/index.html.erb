<h1>Students for <%= @student_container.uniqueSectionCode %> </h1>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
  </tr>

  <% @students.each do |student| %>
      <tr>
        <td><%= student.name.lastSurname %></td><td width="100%"  id="progressBar" data-progress="40"></td>
      </tr>
  <% end %>
</table>

<br />
<%=  debug @students %>

<script type="text/javascript">
  jQuery(function(){
    var progVal = $("#progressBar").data("progress");
    $("#progressBar").progressbar({
      value: progVal
    });
  });
</script>
